modules = ["python-3.11"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["c-ares", "cargo", "freetype", "grpc", "lcms2", "libiconv", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "libyaml", "openjpeg", "openssl", "pkg-config", "rustc", "tcl", "tk", "zlib"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "BootForge GUI"

[[workflows.workflow]]
name = "BootForge GUI"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python main.py --gui"

[workflows.workflow.metadata]
outputType = "console"
